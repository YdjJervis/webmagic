<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="us.codecraft.webmagic.samples.amazon.dao.ImportDataDao">
    <resultMap id="ImportDataMap" type="ImportData">
        <result property="asin" column="asin"/>
        <result property="siteCode" column="siteCode" />
    </resultMap>

    <select id="findBySite" resultMap="ImportDataMap" parameterType="java.lang.String">
        SELECT
          *
        FROM
          test_asin_source
        WHERE
          siteCode = #{0}
    </select>

    <select id="findAll" resultMap="ImportDataMap">
        SELECT
          *
        FROM
          test_asin_source
        limit 10
    </select>
</mapper>