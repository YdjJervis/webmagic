<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="us.codecraft.webmagic.samples.amazon.dao.UrlMonitorDao">

    <resultMap id="UrlMonitorResultMap" type="UrlMonitor">
        <result property="smuUrl" column="smu_url"/>
        <result property="smuStatus" column="smu_status"/>
    </resultMap>

    <select id="findFailureAndUnCrawled" resultMap="UrlMonitorResultMap">
        SELECT * FROM spider_monitor_url WHERE smu_status != 200;
    </select>

</mapper>